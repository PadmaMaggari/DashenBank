		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.MODULEID = inputHeaderRef.MODULEID;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.SERVICE = inputHeaderRef.SERVICE;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.OPERATION = inputHeaderRef.OPERATION;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.SOURCE_USERID = inputHeaderRef.SOURCE_USERID;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.DESTINATION = inputHeaderRef.DESTINATION;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.FUNCTIONID = inputHeaderRef.FUNCTIONID;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.ACTION = inputHeaderRef.ACTION;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_HEADER.MSGSTAT = inputHeaderRef.MSGSTAT;

		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".XREF = inputBodyRef.XREF;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".FCCREF = inputBodyRef.FCCREF;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".PRD = inputBodyRef.PRD;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".BRN = inputBodyRef.BRN;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNBRN = inputBodyRef.TXNBRN;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNACC = inputBodyRef.TXNACC;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNCCY = inputBodyRef.TXNCCY;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNAMT = inputBodyRef.TXNAMT;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNTRN = inputBodyRef.TXNTRN;
