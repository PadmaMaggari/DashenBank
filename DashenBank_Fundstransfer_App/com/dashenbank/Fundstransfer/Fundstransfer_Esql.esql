		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNBRNADDR = inputBodyRef.TXNBRNADDR;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".NARRATIVE = inputBodyRef.NARRATIVE;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".TXNAMTADV = inputBodyRef.TXNAMTADV;
		SET outputbodyRef.Body.ns:RTSERVICEResponse.RT_BODY."Transaction-Details".ACTAMTINWORDS = inputBodyRef.ACTAMTINWORDS;
