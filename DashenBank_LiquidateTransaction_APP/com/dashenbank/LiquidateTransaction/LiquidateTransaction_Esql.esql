		SET OutputRoot.SOAP.Body.*:LIQUIDATETRANSACTION_FSFS_RES.*:FCUBS_BODY.*:"Transaction-Details"."Mis-Details".TXNMIS2LBL2 = InputRoot.SOAP.Body.*:LIQUIDATETRANSACTION_FSFS_RES.*:FCUBS_BODY.*:"Transaction-Details".*:"Mis-Details".*:TXNMIS2LBL2;
		SET OutputRoot.SOAP.Body.*:LIQUIDATETRANSACTION_FSFS_RES.*:FCUBS_BODY.*:FCUBS_WARNING_RESP.WARNING.WCODE = InputRoot.SOAP.Body.*:LIQUIDATETRANSACTION_FSFS_RES.*:FCUBS_BODY.*:FCUBS_WARNING_RESP.*:WARNING.*:WCODE ;
		SET OutputRoot.SOAP.Body.*:LIQUIDATETRANSACTION_FSFS_RES.*:FCUBS_BODY.*:FCUBS_WARNING_RESP.WARNING.WDESC = InputRoot.SOAP.Body.*:LIQUIDATETRANSACTION_FSFS_RES.*:FCUBS_BODY.*:FCUBS_WARNING_RESP.WARNING.*:WDESC;
		SET Environment.OUTSOAP = OutputRoot.SOAP;

		DECLARE outputDomainDataRef REFERENCE TO OutputRoot.SOAP.Body;
		DECLARE outputDomainName CHARACTER 'SOAP';
		DECLARE outputPayload CHARACTER getPayLoad(outputDomainName,outputDomainDataRef,encodeRef,ccidRef);
		SET OutputRoot.SOAP = NULL;

		CREATE LASTCHILD OF OutputRoot DOMAIN 'XMLNSC';
		DECLARE OutRef1 REFERENCE TO OutputRoot.XMLNSC;
		CALL PayloadLogging(Environment.id,outputPayload,'CHANNEL_RESP',MessageFlowLabel,OutRef1);

		SET OutputRoot.Properties = InputRoot.Properties;
		SET OutputRoot.HTTPResponseHeader = InputRoot.HTTPResponseHeader;
		SET OutputRoot.SOAP = Environment.OUTSOAP;

		CALL writeToLogFile(MessageFlowLabel,'LiquidateTransaction_Log','INFO',' ------- FLEXCUBE_RESP -------') INTO rc;
		CALL writeToLogFile(MessageFlowLabel,'LiquidateTransaction_Log','INFO','FLEXCUBE_RESP data is: '||inputPayLoad) INTO rc;
		CALL writeToLogFile(MessageFlowLabel,'LiquidateTransaction_Log','INFO',' ----------------------------------------------------------------------') INTO rc;

		CALL writeToLogFile(MessageFlowLabel,'LiquidateTransaction_Log','INFO',' -------- CHANNEL_RESP -------') INTO rc;
		CALL writeToLogFile(MessageFlowLabel,'LiquidateTransaction_Log','INFO','CHANNEL_RESP data is: '||outputPayload) INTO rc;
		CALL writeToLogFile(MessageFlowLabel,'LiquidateTransaction_Log','INFO','**********************************************************************') INTO rc;	

	END;
END MODULE;
