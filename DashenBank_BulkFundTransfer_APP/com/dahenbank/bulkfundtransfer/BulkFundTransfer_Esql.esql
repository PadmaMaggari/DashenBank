		CREATE LASTCHILD OF OutputRoot DOMAIN 'SOAP';

		SET OutputRoot.SOAP.Context = InputRoot.SOAP.Context;

		CREATE LASTCHILD OF OutputRoot.SOAP NAME 'Body';
		CREATE LASTCHILD OF OutputRoot.SOAP.Body NAMESPACE ns NAME 'CREATEMJRNLBOOK_IOPK_REQ';
		CREATE LASTCHILD OF OutputRoot.SOAP.Body.ns:CREATEMJRNLBOOK_IOPK_REQ NAMESPACE ns NAME 'FCUBS_HEADER';
		CREATE LASTCHILD OF OutputRoot.SOAP.Body.ns:CREATEMJRNLBOOK_IOPK_REQ NAMESPACE ns NAME 'FCUBS_BODY';

